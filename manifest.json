{
	"manifest_version": 2,
	"name": "Dial-up sound",
	"version": "0.1",
	"author": "Ole Steinar L. Skrede",
	"description": "Plays the old schoole dial-up sound when you open your browser",
	"homepage_url": "https://github.com/oleskrede/dial-up-plugin",
	"background": {
		"scripts": [
			"js/background.js"
		]
	},
	"icons": {
		"16": "img/dial-up16.png",
		"32": "img/dial-up32.png"
	},
	"browser_action": {
		"default_icon": "img/dial-up32.png"
	}
}